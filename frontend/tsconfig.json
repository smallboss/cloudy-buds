{
    "compilerOptions": {
        "sourceMap": true,
        "outDir": "./dist/",
        "module": "es6",
        "target": "es2017",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "declarationDir": "./src/types",
        "baseUrl": ".",
        "paths": {
            "cannon" : ["./src/lib/cannon/cannon"]
        }
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
}

